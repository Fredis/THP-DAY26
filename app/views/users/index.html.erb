<h1> Voici la liste des membres: </h1>
<br />
<br />
<table class="table table-hover">
  <tbody>
  	<% @users.each do |user| %>
    <tr>
      <th scope="row"><%= user.id %></th>
      <td><%= user.prenom %></td>
      <td><%= user.nom %></td>
      <td><%= user.email %></td>
    </tr>
		<% end %>
  </tbody>
</table>

